@model IEnumerable<NAA.Data.University>

@{
    ViewBag.Title = "getUniversity";
}

<h2>University List</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UniversityName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UniversityName)
            </td>
            <td>
                @Html.ActionLink("Apply Course", "getCourse_" + item.UniversityId, new { applicantID = ViewBag.ApplicantID, uniID = item.UniversityId, Controller = "UniversityCouse" })
            </td>
        </tr>
    }
</table>
<p>
@Html.ActionLink("Back", "searchUserProfile", "Profiles")
</p>
